{
  "compilerOptions": {
    "esModuleInterop": true,
    "jsx": "react",
    "module": "commonjs",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "skipLibCheck": true,
    "target": "ES2017",
    "types": ["@emotion/react/types/css-prop"]
  },
  "include": [
      "./src/**/*"
  ]
}
